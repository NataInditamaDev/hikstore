{
  "endOfLine": "auto",
  "importOrder": ["<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "", "^@/(.*)$", "^[./]", "", "<TYPES>", "<TYPES>^[./]"],
  "plugins": ["prettier-plugin-sort-json", "@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5"
}
